extends layout

block content
	div.container
		div.row
			div.col-sm-6.col-md-4.col-md-offset-4
				#user
					h1.text-center.login-title Twoje pliki
						div.signup-wall
							ul.user-details
							each file in files
								input(type='radio', name='filer', value='#{file._id}')
								p #{file.filename}
								a.delete(href="/home/openbox/delete/" + file._id)="Delete Offer"
